# Revi Monitor SDK Configuration

# API Configuration
REVI_API_URL=https://api.revi.dev
REVI_ENVIRONMENT=production

# Debug Configuration
REVI_DEBUG=false

# Sampling Configuration
REVI_ERROR_SAMPLE_RATE=1.0
REVI_SESSION_SAMPLE_RATE=0.1
REVI_PERFORMANCE_SAMPLE_RATE=0.1
REVI_NETWORK_SAMPLE_RATE=0.1
REVI_REPLAY_SAMPLE_RATE=0.1

# Development Configuration (for local development)
# REVI_API_URL=http://localhost:4000
# REVI_ENVIRONMENT=development
# REVI_DEBUG=true
# REVI_ERROR_SAMPLE_RATE=1.0
# REVI_SESSION_SAMPLE_RATE=1.0
# REVI_PERFORMANCE_SAMPLE_RATE=1.0
# REVI_NETWORK_SAMPLE_RATE=1.0
# REVI_REPLAY_SAMPLE_RATE=1.0

# Release Information
REVI_RELEASE_VERSION=1.0.0
REVI_BUILD_HASH=

# Privacy Configuration
REVI_MASK_INPUTS=true
REVI_MASK_PASSWORDS=true
REVI_MASK_CREDIT_CARDS=true
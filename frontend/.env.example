# Revi Frontend Demo App Environment Configuration

# Backend API Configuration
VITE_CLIENT_TARGET=http://localhost:4000
VITE_API_BASE_URL=http://localhost:4000

# Revi SDK Configuration
VITE_REVI_API_KEY=your-project-api-key-here
VITE_REVI_PROJECT_ID=your-project-id-here
VITE_REVI_ENVIRONMENT=development

# Demo App Configuration
VITE_APP_NAME=Revi Demo App
VITE_APP_VERSION=1.0.0
VITE_DEBUG_MODE=true

# Feature Flags
VITE_ENABLE_SESSION_REPLAY=true
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_ENABLE_NETWORK_MONITORING=true
VITE_ENABLE_BREADCRUMBS=true

# Demo Configuration
VITE_DEMO_USER_ID=demo-user-123
VITE_DEMO_SESSION_ID=demo-session-456
VITE_AUTO_GENERATE_ERRORS=false
VITE_ERROR_SIMULATION_INTERVAL=30000

# Development Settings
VITE_SHOW_DEBUG_PANEL=true
VITE_LOG_LEVEL=debug
VITE_MOCK_DATA=false